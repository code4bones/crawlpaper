IDM_MENU MENU 
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "&Open with Associated Program...", IDM_FILE_OPEN
        MENUITEM "Save &As...",                 IDM_FILE_SAVEAS
        MENUITEM "&Delete",                     IDM_FILE_DELETE
        MENUITEM SEPARATOR
        MENUITEM "E&xit\tAlt+F4",               IDM_FILE_EXIT
    END
    POPUP "&Edit"
    BEGIN
        MENUITEM "&Copy\tCtrl+C",               IDM_EDIT_COPY
        MENUITEM "&Paste\tCtrl+V",              IDM_EDIT_PASTE
        MENUITEM SEPARATOR
        MENUITEM "&Empty Clipboard\tCtrl+E",    IDM_EDIT_EMPTYCLIPBOARD
    END
    POPUP "&Playlist"
    BEGIN
        MENUITEM "&Save Current Playlist\tF4",  IDM_PLAYLIST_SAVEPLAYLIST
        MENUITEM SEPARATOR
        MENUITEM "&Previous",                   IDM_PLAYLIST_PREVIOUS
        MENUITEM "&Next",                       IDM_PLAYLIST_NEXT
        MENUITEM "Pa&use",                      IDM_PLAYLIST_PAUSE
        MENUITEM "&Resume",                     IDM_PLAYLIST_RESUME
        MENUITEM SEPARATOR
        MENUITEM "Add &File...\tF5",            IDM_PLAYLIST_ADDFILE
        MENUITEM "Add &Dir...\tF6",             IDM_PLAYLIST_ADDDIR
        MENUITEM "Add &Url...",                 IDM_PLAYLIST_ADDURL
        MENUITEM "Re&move\tF7",                 IDM_PLAYLIST_REMOVE
        MENUITEM "Remove &All\tF8",             IDM_PLAYLIST_REMOVEALL
        MENUITEM SEPARATOR
        MENUITEM "&Unload Current Playlist (.dpl)\tShift+F8", IDM_PLAYLIST_UNLOADLIST
        MENUITEM "Ne&w Playlist (.dpl)...\tShift+F9", IDM_PLAYLIST_NEWLIST
        MENUITEM "&Load Playlist (.dpl)...\tShift+F10", IDM_PLAYLIST_LOADLIST
        MENUITEM "&Save Playlist (.dpl)...\tShift+F11", IDM_PLAYLIST_SAVELIST
        MENUITEM SEPARATOR
        MENUITEM "Add to &Favourite Pictures",  IDM_PLAYLIST_FAVOURITE_PICTURE
        MENUITEM "Add to &Favourite Audio",     IDM_PLAYLIST_FAVOURITE_AUDIO
        POPUP "Load &Favourite..."
        BEGIN
            MENUITEM "&Pictures",                   IDM_PLAYLIST_LOAD_FAVOURITE_PICTURE
            MENUITEM "&Audio",                      IDM_PLAYLIST_LOAD_FAVOURITE_AUDIO
        END
    END
    POPUP "&Tools"
    BEGIN
        MENUITEM "Remove Current Wall&Paper",   IDM_TOOLS_UNDESKTOP
        MENUITEM "&Hide/Show Desktop Icons",    IDM_TOOLS_DESKTOPICONS
        MENUITEM "&Display Properties...",      IDM_TOOLS_DISPLAYPROPERTIES
        MENUITEM SEPARATOR
        MENUITEM "&Uncompress...\tCtrl+F8",     IDM_TOOLS_UNCOMPRESS
        MENUITEM "Picture &Browser...\tCtrl+F9", IDM_TOOLS_WALLBROWSER
        MENUITEM "&Thumbnailer...\tCtrl+F10",   IDM_TOOLS_THUMBNAILS
        MENUITEM "C&rawler...\tCtrl+F11",       IDM_TOOLS_CRAWLER
    END
    POPUP "&Options"
    BEGIN
        POPUP "General..."
        BEGIN
            MENUITEM "Auto Run",                    IDM_OPTIONS_AUTORUN
            MENUITEM "Clear ""Don't ask me more"" Flags", IDM_OPTIONS_CLEAR_DONOTASKMORE
            MENUITEM "Context Menu for Supported Formats", IDM_OPTIONS_CONTEXT_MENU
            MENUITEM "Recurse Subdir",              IDM_OPTIONS_RECURSE_SUBDIR
            MENUITEM "Confirm File Delete",         IDM_OPTIONS_CONFIRM_FILE_DELETE
            MENUITEM "Delete Files to Recycle Bin", IDM_OPTIONS_DELETE_FILES_TO_RECYCLEBIN
            POPUP "Clear Temporary Files..."
            BEGIN
                MENUITEM "At Exit",                     IDM_OPTIONS_TEMPORARY_FILES_CLEAR_AT_EXIT
                MENUITEM "At Runtime",                  IDM_OPTIONS_TEMPORARY_FILES_CLEAR_AT_RUNTIME
                MENUITEM "Do Not Clear",                IDM_OPTIONS_TEMPORARY_FILES_DO_NOT_CLEAR
            END
            POPUP "Process Priority..."
            BEGIN
                MENUITEM "Real Time",                   IDM_OPTIONS_PRIORITY_PROCESS_REALTIME
                MENUITEM "High",                        IDM_OPTIONS_PRIORITY_PROCESS_HIGH
                MENUITEM "Normal",                      IDM_OPTIONS_PRIORITY_PROCESS_NORMAL
                MENUITEM "Idle",                        IDM_OPTIONS_PRIORITY_PROCESS_IDLE
            END
            POPUP "Threads Priority..."
            BEGIN
                MENUITEM "Time Critical",               IDM_OPTIONS_PRIORITY_THREAD_TIME_CRITICAL
                MENUITEM "Highest",                     IDM_OPTIONS_PRIORITY_THREAD_HIGHEST
                MENUITEM "Above Normal",                IDM_OPTIONS_PRIORITY_THREAD_ABOVE_NORMAL
                MENUITEM "Normal",                      IDM_OPTIONS_PRIORITY_THREAD_NORMAL
                MENUITEM "Below Normal",                IDM_OPTIONS_PRIORITY_THREAD_BELOW_NORMAL
                MENUITEM "Lowest",                      IDM_OPTIONS_PRIORITY_THREAD_LOWEST
                MENUITEM "Idle",                        IDM_OPTIONS_PRIORITY_THREAD_IDLE
            END
        END
        POPUP "User Interface..."
        BEGIN
            MENUITEM "Always on Top",               IDM_OPTIONS_ALWAYSONTOP
            MENUITEM "Splash Screen",               IDM_OPTIONS_SPLASHSCREEN
            MENUITEM "Tray Icon",                   IDM_OPTIONS_TRAYICON
            MENUITEM "Change with one Click on Tray Icon", IDM_OPTIONS_ONE_CLICK_CHANGE
            MENUITEM "Minimize to Taskbar/Tray on Close Window", IDM_OPTIONS_MINIMIZE_ON_CLOSE
            POPUP "Grid..."
            BEGIN
                MENUITEM "Gridlines",                   IDM_OPTIONS_GRIDLINES
                MENUITEM SEPARATOR
                MENUITEM "Foreground Color...",         IDM_OPTIONS_GRIDLINES_FOREGROUND_COLOR
                MENUITEM "Background Color...",         IDM_OPTIONS_GRIDLINES_BACKGROUND_COLOR
                MENUITEM SEPARATOR
                MENUITEM "Reset to Default",            IDM_OPTIONS_GRID_RESET
            END
            POPUP "Popups..."
            BEGIN
                POPUP "Audio..."
                BEGIN
                    MENUITEM "None",                        IDM_OPTIONS_AUDIO_NO_POPUP
                    MENUITEM SEPARATOR
                    MENUITEM "Cartoons Balloon",            IDM_OPTIONS_AUDIO_BALLOON_POPUP
                    MENUITEM "Reload Balloon...",           IDM_OPTIONS_AUDIO_RELOAD_BALLOON
                    POPUP "Balloon Icon..."
                    BEGIN
                        MENUITEM "Default",                     IDM_OPTIONS_AUDIO_BALLOON_DEFAULTICON
                        MENUITEM "Load From Directory...",      IDM_OPTIONS_AUDIO_BALLOON_CUSTOMICON
                    END
                    MENUITEM SEPARATOR
                    MENUITEM "Taskbar Bitmap",              IDM_OPTIONS_AUDIO_TASKBAR_POPUP
                    MENUITEM "Reload Bitmap List...",       IDM_OPTIONS_AUDIO_RELOAD_TASKBAR_POPUP
                END
                POPUP "Pictures..."
                BEGIN
                    MENUITEM "None",                        IDM_OPTIONS_PICT_NO_POPUP
                    MENUITEM SEPARATOR
                    MENUITEM "Cartoons Balloon",            IDM_OPTIONS_PICT_BALLOON_POPUP
                    MENUITEM "Reload Balloon...",           IDM_OPTIONS_PICT_RELOAD_BALLOON
                    POPUP "Balloon Icon..."
                    BEGIN
                        MENUITEM "Default",                     IDM_OPTIONS_PICT_BALLOON_DEFAULTICON
                        MENUITEM "Load From Directory...",      IDM_OPTIONS_PICT_BALLOON_CUSTOMICON
                    END
                    MENUITEM SEPARATOR
                    MENUITEM "Taskbar Bitmap",              IDM_OPTIONS_PICT_TASKBAR_POPUP
                    MENUITEM "Reload Bitmap List...",       IDM_OPTIONS_PICT_RELOAD_TASKBAR_POPUP
                END
            END
            POPUP "Nervous Title..."
            BEGIN
                MENUITEM "Yes, I'm a Restless Guy",     IDM_OPTIONS_NERVOUS_TITLE
                MENUITEM "(In)Famous Sentences",        IDM_OPTIONS_INFAMOUS_SENTENCES
            END
        END
        POPUP "Playlist..."
        BEGIN
            POPUP "Loading Mode..."
            BEGIN
                MENUITEM "Add .dpl Content to the Current Playlist", IDM_OPTIONS_PLAYLIST_LOADING_ADD
                MENUITEM "Load .dpl Content Removing Existing Items", IDM_OPTIONS_PLAYLIST_LOADING_PUT
            END
            POPUP "Randomize..."
            BEGIN
                MENUITEM "None (Play as Loaded)",       IDM_OPTIONS_RANDOMIZE_PLAYLIST_NONE
                MENUITEM "Randomize Item Selection",    IDM_OPTIONS_RANDOMIZE_PLAYLIST_SELECTION
                MENUITEM "Do a Random Reorder Now...",  IDM_OPTIONS_RANDOMIZE_PLAYLIST_REORDER
            END
        END
        MENUITEM SEPARATOR
        MENUITEM "&Draw Settings...",           IDM_OPTIONS_DRAW_SETTINGS
        MENUITEM "&Thumbnails Settings...",     IDM_OPTIONS_THUMBNAILS_SETTINGS
        MENUITEM "&Crawler Settings...",        IDM_OPTIONS_CRAWLER_SETTINGS
        MENUITEM SEPARATOR
        MENUITEM "&Save Configuration to File...", IDM_OPTIONS_SAVE_CONFIG
        MENUITEM "&Load Configuration from File...", IDM_OPTIONS_LOAD_CONFIG
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "Hotkey(&s)...",               IDM_HELP_HOTKEYS
        MENUITEM "&Home Page...\tShift+F1",     IDM_HELP_HOMEPAGE
        POPUP "&Latest Version..."
        BEGIN
            MENUITEM "&Check Now...",               IDM_HELP_LATESTVERSION
            MENUITEM SEPARATOR
            MENUITEM "&At Startup",                 IDM_HELP_LATESTVERSION_AT_STARTUP
            MENUITEM "&Every 15 Day(s)",            IDM_HELP_LATESTVERSION_EVERY_N_DAYS
            MENUITEM "&Never",                      IDM_HELP_LATESTVERSION_NEVER
        END
        MENUITEM SEPARATOR
        MENUITEM "&About...",                   IDM_HELP_ABOUT
    END
END

